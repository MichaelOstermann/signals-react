{
    "name": "@monstermann/unplugin-signals-react",
    "type": "module",
    "version": "0.2.2",
    "description": "Transformer plugin for signals-react.",
    "author": "Michael Ostermann <michaelostermann@me.com>",
    "license": "MIT",
    "homepage": "https://MichaelOstermann.github.io/react-signals",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MichaelOstermann/react-signals.git"
    },
    "bugs": {
        "url": "https://github.com/MichaelOstermann/react-signals/issues"
    },
    "keywords": [],
    "sideEffects": false,
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./esbuild": {
            "import": {
                "types": "./dist/esbuild.d.mts",
                "default": "./dist/esbuild.mjs"
            },
            "require": {
                "types": "./dist/esbuild.d.cts",
                "default": "./dist/esbuild.cjs"
            }
        },
        "./rolldown": {
            "import": {
                "types": "./dist/rolldown.d.mts",
                "default": "./dist/rolldown.mjs"
            },
            "require": {
                "types": "./dist/rolldown.d.cts",
                "default": "./dist/rolldown.cjs"
            }
        },
        "./rollup": {
            "import": {
                "types": "./dist/rollup.d.mts",
                "default": "./dist/rollup.mjs"
            },
            "require": {
                "types": "./dist/rollup.d.cts",
                "default": "./dist/rollup.cjs"
            }
        },
        "./rspack": {
            "import": {
                "types": "./dist/rspack.d.mts",
                "default": "./dist/rspack.mjs"
            },
            "require": {
                "types": "./dist/rspack.d.cts",
                "default": "./dist/rspack.cjs"
            }
        },
        "./types": {
            "import": {
                "types": "./dist/types.d.mts",
                "default": "./dist/types.mjs"
            },
            "require": {
                "types": "./dist/types.d.cts",
                "default": "./dist/types.cjs"
            }
        },
        "./vite": {
            "import": {
                "types": "./dist/vite.d.mts",
                "default": "./dist/vite.mjs"
            },
            "require": {
                "types": "./dist/vite.d.cts",
                "default": "./dist/vite.cjs"
            }
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./*"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "bundle": "tsdown",
        "release": "bun publish --access public"
    },
    "dependencies": {
        "@monstermann/signals-react": "workspace:*",
        "magic-string": "^0.30.17",
        "oxc-parser": "^0.101.0",
        "oxc-walker": "^0.6.0",
        "unplugin": "^2.3.4"
    },
    "devDependencies": {
        "redent": "^4.0.0"
    }
}
